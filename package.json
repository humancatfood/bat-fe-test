{
  "name": "bookatable-fe-test-2017-03",
  "version": "1.1.0",
  "description": "Basic table booking manager prototype",
  "scripts": {
    "start": "node ./bin/www",
    "prestart": "npm run build",
    "dev:client": "react-scripts start",
    "dev:server": "cross-env PORT=3001 NODE_ENV=development nodemon --inspect ./bin/www",
    "dev": "concurrently -n server,client -c 'yellow,magenta' npm:dev:server npm:dev:client",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "STATIC_PATH=/bat-fe-test/ gh-pages-deploy"
  },
  "proxy": "http://localhost:3001",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "repository": {
    "private": true
  },
  "devDependencies": {
    "@testing-library/react": "^9.4.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "gh-pages-deploy": "^0.4.2",
    "node-sass": "^4.11.0",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.7",
    "classnames": "^2.2.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "graphql": "^14.6.0",
    "graphql-yoga": "^1.18.3",
    "graphql.macro": "^1.4.2",
    "lodash": "^4.17.15",
    "morgan": "~1.9.1",
    "nedb": "^1.8.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "uuid": "^3.0.1",
    "yup": "^0.28.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ]
  }
}
